{
  "name": "react-tiny-router",
  "version": "2.0.0",
  "description": "A tiny React router",
  "main": "router.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore '**/__tests__/**'",
    "lint": "chq-scripts lint",
    "test": "chq-scripts test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CultureHQ/react-tiny-router.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CultureHQ/react-tiny-router/issues"
  },
  "homepage": "https://github.com/CultureHQ/react-tiny-router#readme",
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@culturehq/scripts": "^0.2.0",
    "babel-plugin-macros": "^2.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^7.0.1"
  },
  "eslintConfig": {
    "extends": "@culturehq",
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "TinyRouter"
        }
      ]
    }
  },
  "keywords": [
    "babel-plugin-macros",
    "react"
  ]
}
